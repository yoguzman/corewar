live	param 1 = 4octets						
ld		param 1 = (dir ou ind)inversion			param 2 = normal
st		param 1 = normal						param 2 = (ind reg)inversion
add		normal
sub		normal
and 	param 1 = tout (inversion si pa registre)	param 2 = pareil param 1
or		""
